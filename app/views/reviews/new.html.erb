<%= form_with(model:[@shop, @review], method: :post, local: true) do |f| %>
  <div class="row">
    <div class="col-sm-12">
      <%= f.text_area :review, rows:'5', class: "form-control",placeholder: "コメントをここに" %>
    </div>
  </div>
  <%= f.submit "送信する", class: "btn btn-lg btn-base-1 mt-20 pull-right" %>
<% end %>